# === Chatwoot API ===
CHATWOOT_BASE_URL=https://chatwoot.moonrein.net
CHATWOOT_ACCOUNT_ID=1
CHATWOOT_INBOX_ID=1

# Devise Token Auth
CHATWOOT_ACCESS_TOKEN=your-chatwoot-access-token
CHATWOOT_CLIENT=your-chatwoot-client
CHATWOOT_UID=your-chatwoot-uid

# === Discord ===
DISCORD_BOT_TOKEN=your-discord-bot-token-here
DISCORD_GUILD_ID=your_server_id_here
DISCORD_CHANNEL_ID=1379031069768024145

# === Webhook (Flask) ===
FLASK_SERVER_URL=https://discordbot.moonrein.net/create_contact

# === Logging ===
LOG_LEVEL=INFO
LOG_JSON=true
LOG_BODY_MAX=2000
REDACT_KEYS=authorization,api_access_token,access-token,client,uid,token,secret,password

# === File Proxy ===
FILE_PROXY_FETCH_BASE=http://127.0.0.1:5500
PUBLIC_HOST=https://chatwoot.moonrein.net
INTERNAL_FETCH_BASE=https://chatwoot.moonrein.net
HOST_REWRITE_MAP=https://172.18.0.1->https://chatwoot.moonrein.net
